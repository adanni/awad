<h1>Users#index</h1>
<% provide(:title, 'All User Page') %>
<ul class="users">
	<% @users.each do |user| %>
	 <li><%= link_to "#{(email_to_name(user)).titlecase}'s Profile", user_path(user), {style: "color:blue" }  %>
     <% if current_user.admin %>
     <%= link_to " Delete Account", user, method: :delete, data: { confirm: "Are you sure?"}, class: "btn" %>
     <%= link_to " Edit Account", edit_user_path(user), class: "btn" %>
     <% end %>
     </li>
     <br />
  <% end %>
</ul>

